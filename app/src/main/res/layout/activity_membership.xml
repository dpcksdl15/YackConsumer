                android:layout_height="match_parent"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/tv_provision2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nr"
                android:text="내용보기"
                android:textColor="#979797"
                android:textSize="12sp" />

        </LinearLayout>

        <Button
            android:id="@+id/bt_memberinfo_enter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="20dp"
            android:text="가 입 하 기"
            android:textColor="@color/white"
            android:fontFamily="@font/neb"
            android:textSize="14sp"
            android:background="@drawable/backgound_md_price_search_button"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/check2" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>